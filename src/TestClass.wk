import TestMethod;

every TestClass is:

	provides TestMethod:Internal;

	with Text name = "";
	with TestMethod[] = [];

	Text -- getName() {
		return name;
	}

	addMethod(Text mname) {
		var $TestMethod:Internal <- this;
		$TestMethod.name = mname;
		TestMethod[TestMethod.getSize()] = $TestMethod;
	}

	TestMethod[] -- getMethods() {
		return TestMethod;
	}
