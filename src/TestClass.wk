import TestMethod;

every TestClass is:

	needs Text name;

	provides TestMethod, Text <- { return addingmethodname; };
	with Text addingmethodname = "";

	with TestMethod[] = [];

	Text -- getName() {
		return name;
	}

	addMethod(Text mname) {
		addingmethodname = mname;
		var $TestMethod <- this;
		TestMethod[TestMethod.getSize()] = $TestMethod;
	}

	TestMethod[] -- getMethods() {
		return TestMethod;
	}
